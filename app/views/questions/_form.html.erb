<%= simple_form_for(@question) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :questionContent %>
    <%= f.input :answerType %>
    <%= f.association :category, as: :radio_buttons, item_wrapper_class: 'inline'%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
